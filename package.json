{
  "name": "empress",
  "version": "1.0.0",
  "description": "A blazing fast and multipurpose discord bot with wide range of features",
  "main": "dist/bot.js",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "prestart": "npm run build",
    "start": "node --enable-source-maps dist/bot.js",
    "dev": "concurrently \"tsc -w\" \"nodemon dist/bot.js\"",
    "start:pm2": "npm run build && npm run pm2:start",
    "pm2:start": "pm2 start process.json",
    "pm2:stop": "pm2 stop all",
    "pm2:restart": "pm2 restart all",
    "pm2:delete": "pm2 delete all",
    "pm2:logs": "pm2 logs"
  },
  "keywords": [
    "discord",
    "bot"
  ],
  "author": "Aditya",
  "license": "MIT",
  "dependencies": {
    "chart.js": "^4.4.7",
    "chartjs-plugin-gradient": "^0.5.0",
    "discord.js": "^14.17.3",
    "discord.js-rate-limiter": "^1.3.2",
    "dotenv": "^16.4.7",
    "glob": "^11.0.1",
    "mongoose": "^8.9.6",
    "node-fetch": "^3.3.2",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
