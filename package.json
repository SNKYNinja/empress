{
  "name": "empress",
  "version": "2.0.0",
  "description": "A blazing fast and multipurpose discord bot with wide range of features",
  "main": "dist/bot.js",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && tsc",
    "dev": "nodemon",
    "start": "pm2 start process.json",
    "stop": "pm2 delete all",
    "restart": "pm2 restart all",
    "logs": "pm2 logs",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,json,md,yml,yaml}\"",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepare": "lefthook install"
  },
  "keywords": [
    "discord",
    "bot"
  ],
  "author": "Aditya Chaudhary",
  "license": "MIT",
  "dependencies": {
    "@discordjs/builders": "^1.11.2",
    "chart.js": "^4.5.0",
    "chartjs-plugin-gradient": "^0.5.1",
    "discord.js": "^14.21.0",
    "discord.js-rate-limiter": "^1.3.2",
    "dotenv": "^16.6.1",
    "glob": "^11.0.3",
    "mongoose": "^8.16.1",
    "node-fetch": "^3.3.2",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "pm2": "^6.0.8",
    "poru": "^5.2.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
